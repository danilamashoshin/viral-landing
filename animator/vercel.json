{
  "functions": {
    "api/*.js": { "runtime": "nodejs18.x" },
    "api/**/*.js": { "runtime": "nodejs18.x" }
  },
  "routes": [
    { "src": "/webhooks/lemon", "dest": "api/lemon.js" },
    { "src": "/events/lead", "dest": "api/lead.js" },
    { "src": "/api/health", "dest": "/health.json" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
