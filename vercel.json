   {
     "functions": {
       "api/*.js": {
         "runtime": "nodejs18.x"
       }
     },
     "rewrites": [
       { "source": "/api/health", "destination": "/health.json" },
       { "source": "/events/lead", "destination": "/api/lead" },
       { "source": "/webhooks/lemon", "destination": "/api/lemon" },
       { "source": "/(.*)", "destination": "/animator/$1" }
     ]
   }
